{
  "name": "fastmcp-token-generator",
  "version": "1.0.0",
  "description": "FastMCP JWT Token Generator using jsonwebtoken",
  "main": "token-generator.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/token-generator.js",
    "start:client": "node dist/real-client.js",
    "token": "node dist/token-generator.js",
    "client": "node dist/real-client.js",
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "jwt",
    "token",
    "fastmcp",
    "authentication"
  ],
  "author": "",
  "license": "MIT"
} 
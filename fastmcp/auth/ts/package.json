  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {},
  "optionalDependencies": {
    "@langchain/mcp-adapters": "^0.0.1",
    "@langchain/openai": "^0.0.1",

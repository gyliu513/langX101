  "description": "FastMCP Authentication TypeScript Implementation",
  "type": "module",
  "main": "dist/token-generator.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/token-generator.js",
    "start:client": "node dist/real-client.js",
    "token": "node dist/token-generator.js",
    "client": "node dist/real-client.js",
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "type-check": "tsc --noEmit",
